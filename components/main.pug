mixin video-item(video, img, title, chname, info)
    .item 
        .item-video
            iframe(src=video)

        .item-description 
            .img-channel 
                img(src=img)
            .item-text
                h3= title

                p= chname

                .item-info
                    p= info

mixin hello(name, age)
    h1= name 
    p= age


